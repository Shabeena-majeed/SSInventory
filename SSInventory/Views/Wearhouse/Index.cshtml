@model IEnumerable<SSInventory.Models.Wearhouse>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
   <a asp-action="WearhouseForm" class="btn-primary btn">Add New</a>
</p>
@if (Model.Count() > 0)
{
<table class="table">
    <thead>
        <tr>
          
            <th>
                @Html.DisplayNameFor(model => model.varName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.varDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
          
            <td>
                @Html.DisplayFor(modelItem => item.varName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.varDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isActive)
            </td>
            <td>
         <a asp-action="WearhouseForm" asp-route-intWearhouseid='@item.intSeqId' class="btn-primary btn">Edit</a>
                        <a asp-action="DeleteInfo"  asp-route-intWearhouseid='@item.intSeqId' class="btn-primary btn">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
}
else
{
    <div class="alert alert-info">
        <p>No Record in database</p>

    </div>
}
